{
  "name": "soundbrett",
  "version": "1.0.4",
  "description": "Soundbrett Frounted",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc",
    "start": "npm run compile && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder build -w",
    "release": "npm run build --publish always"
  },
  "author": "threor",
  "repository": "github:Threor/soundboard-frontend",
  "bugs": "https://github.com/Threor/soundboard-frontend/issues",
  "license": "ISC",
  "dependencies": {
    "asar": "^3.0.3",
    "electron-log": "^4.3.0",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "form-data": "^3.0.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1"
  },
  "build": {
    "productName": "Soundbrett",
    "win": {
      "publish": "github",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  }
}
